import random

numeroale = random.randint(1, 100)

while True:
    numeroescolhido = int(input("Escolha um numero entre 1 e 100 "))
    if numeroescolhido < numeroale:
        print("Esta longe!")
    elif numeroescolhido > numeroale:
        print("Esta perto!")
    else:
        print("Parabens, voce acertou!")
        break